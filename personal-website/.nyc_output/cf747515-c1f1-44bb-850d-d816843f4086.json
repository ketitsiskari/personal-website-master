mail) => {\r\n  return new Promise((resolve) => {\r\n    for (const ending of VALID_EMAIL_ENDINGS) {\r\n      if (email.endsWith(ending)) {\r\n        resolve(true);\r\n        return;\r\n      }\r\n    }\r\n    resolve(false);\r\n  });\r\n};\r\n\r\nconst validateWithThrow = (email) => {\r\n  for (const ending of VALID_EMAIL_ENDINGS) {\r\n    if (email.endsWith(ending)) {\r\n      return true;\r\n    }\r\n  }\r\n\r\n  throw new Error('Invalid email');\r\n};\r\n\r\nconst validateWithLog = (email) => {\r\n  for (const ending of VALID_EMAIL_ENDINGS) {\r\n    if (email.endsWith(ending)) {\r\n      console.log(`${email} ends with ${ending}`);\r\n      return true;\r\n    }\r\n  }\r\n\r\n  console.log(`${email} does not have a valid ending`);\r\n  return false;\r\n};\r\n\r\nexport default { validate, validateAsync, validateWithThrow, validateWithLog };\r\n"],"mappings":";;;;;;AAAA,IAAMA,mBAAmB,GAAG,CAAC,WAAW,EAAE,aAAa,EAAE,WAAW,CAAC;AAErE,IAAMC,QAAQ,GAAG,SAAXA,QAAQA,CAAIC,KAAK,EAAK;EAC1B,SAAAC,EAAA,MAAAC,oBAAA,GAAqBJ,mBAAmB,EAAAG,EAAA,GAAAC,oBAAA,CAAAC,MAAA,EAAAF,EAAA,IAAE;IAArC,IAAMG,MAAM,GAAAF,oBAAA,CAAAD,EAAA;IACf,IAAID,KAAK,CAACK,QAAQ,CAACD,MAAM,CAAC,EAAE;MAC1B,OAAO,IAAI;IACb;EACF;EACA,OAAO,KAAK;AACd,CAAC;AAED,IAAME,aAAa,GAAG,SAAhBA,aAAaA,CAAIN,KAAK,EAAK;EAC/B,OAAO,IAAIO,OAAO,CAAC,UAACC,OAAO,EAAK;IAC9B,SAAAC,GAAA,MAAAC,qBAAA,GAAqBZ,mBAAmB,EAAAW,GAAA,GAAAC,qBAAA,CAAAP,MAAA,EAAAM,GAAA,IAAE;MAArC,IAAML,MAAM,GAAAM,qBAAA,CAAAD,GAAA;MACf,IAAIT,KAAK,CAACK,QAAQ,CAACD,MAAM,CAAC,EAAE;QAC1BI,OAAO,CAAC,IAAI,CAAC;QACb;MACF;IACF;IACAA,OAAO,CAAC,KAAK,CAAC;EAChB,CAAC,CAAC;AACJ,CAAC;AAED,IAAMG,iBAAiB,GAAG,SAApBA,iBAAiBA,CAAIX,KAAK,EAAK;EACnC,SAAAY,GAAA,MAAAC,qBAAA,GAAqBf,mBAAmB,EAAAc,GAAA,GAAAC,qBAAA,CAAAV,MAAA,EAAAS,GAAA,IAAE;IAArC,IAAMR,MAAM,GAAAS,qBAAA,CAAAD,GAAA;IACf,IAAIZ,KAAK,CAACK,QAAQ,CAACD,MAAM,CAAC,EAAE;MAC1B,OAAO,IAAI;IACb;EACF;EAEA,MAAM,IAAIU,KAAK,CAAC,eAAe,CAAC;AAClC,CAAC;AAED,IAAMC,eAAe,GAAG,SAAlBA,eAAeA,CAAIf,KAAK,EAAK;EACjC,SAAAgB,GAAA,MAAAC,qBAAA,GAAqBnB,mBAAmB,EAAAkB